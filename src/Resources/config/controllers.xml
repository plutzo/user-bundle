<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="Marlinc\UserBundle\Controller\AdminSecurityController" public="true">
            <call method="setContainer">
                <argument type="service" id="service_container" />
            </call>
        </service>
        <service id="Marlinc\UserBundle\Controller\AdminResettingController" public="true">
            <argument type="service" id="event_dispatcher" />
            <argument type="service" id="marlinc.user.resetting.form.factory" />
            <argument type="service" id="Marlinc\UserBundle\Manager\UserManager" />
            <argument type="service" id="marlinc.user.util.token_generator" />
            <argument type="service" id="marlinc.user.mailer.admin" />
            <argument>%marlinc.user.resetting.retry_ttl%</argument>
            <call method="setContainer">
                <argument type="service" id="service_container" />
            </call>
        </service>
    </services>
</container>