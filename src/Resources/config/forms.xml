<?xml version="1.0" encoding="utf-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="Marlinc\UserBundle\Form\PersonFormType">
            <tag name="form.type" />
        </service>
        <service id="marlinc.user.registration.form.factory" class="Marlinc\UserBundle\Form\Factory\FormFactory">
            <argument type="service" id="form.factory" />
            <argument>marlinc_user_registration_form</argument>
            <argument>Marlinc\UserBundle\Form\Type\RegistrationFormType</argument>
            <argument type="collection">
                <argument>Default</argument>
                <argument>Registration</argument>
            </argument>
        </service>
        <service id="marlinc.user.registration.form.type" class="Marlinc\UserBundle\Form\Type\RegistrationFormType">
            <argument>%marlinc.user.class.user%</argument>
            <tag name="form.type" />
        </service>
        <service id="marlinc.user.resetting.form.factory" class="Marlinc\UserBundle\Form\Factory\FormFactory">
            <argument type="service" id="form.factory" />
            <argument>marlinc_user_resetting_form</argument>
            <argument>Marlinc\UserBundle\Form\Type\ResettingFormType</argument>
            <argument type="collection">
                <argument>ResetPassword</argument>
            </argument>
        </service>
        <service id="marlinc.user.resetting.form.type" class="Marlinc\UserBundle\Form\Type\ResettingFormType">
            <argument>%marlinc.user.class.user%</argument>
            <tag name="form.type" />
        </service>
        <service id="marlinc.user.profile.form.factory" class="Marlinc\UserBundle\Form\Factory\FormFactory">
            <argument type="service" id="form.factory" />
            <argument>marlinc_user_profile_form</argument>
            <argument>Marlinc\UserBundle\Form\Type\ProfileFormType</argument>
            <argument type="collection">
                <argument>Default</argument>
                <argument>Profile</argument>
            </argument>
        </service>
        <service id="marlinc.user.profile.form.type" class="Marlinc\UserBundle\Form\Type\ProfileFormType">
            <argument>%marlinc.user.class.user%</argument>
            <tag name="form.type" />
        </service>
        <service id="marlinc.user.change_password.form.factory" class="Marlinc\UserBundle\Form\Factory\FormFactory">
            <argument type="service" id="form.factory" />
            <argument>marlinc_user_change_password_form</argument>
            <argument>Marlinc\UserBundle\Form\Type\ChangePasswordFormType</argument>
            <argument type="collection">
                <argument>ChangePassword</argument>
            </argument>
        </service>
        <service id="marlinc.user.change_password.form.type" class="Marlinc\UserBundle\Form\Type\ChangePasswordFormType">
            <argument>%marlinc.user.class.user%</argument>
            <tag name="form.type" />
        </service>
    </services>
</container>